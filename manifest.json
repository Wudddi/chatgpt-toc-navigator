{
  "manifest_version": 3,
  "name": "ChatGPT TOC Navigator",
  "version": "0.1.0",
  "description": "Adds a clickable table of contents to ChatGPT conversations.",
  "permissions": ["storage"],
  "host_permissions": ["https://chatgpt.com/*", "https://chat.openai.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*", "https://chat.openai.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ]
}
